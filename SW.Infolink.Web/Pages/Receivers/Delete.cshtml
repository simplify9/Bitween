@page "{id:int}"
@model  SW.Infolink.Web.Pages.Receivers.DeleteModel
@{
}

<div class="card card-default">
    <div class="card-body">
        <form id="profile-form" method="post">
            <div asp-validation-summary="All" class="text-danger"></div>
            <div class="form-group">
                <p>Do you want to delete Receiver with name @ViewData["ReceiverName"]?</p>
            </div>
            <button type="submit" class="btn btn-default">Yes</button>
            <a asp-page="/Receivers/Index" class="btn btn-secondary" role="button">No</a>

        </form>
    </div>
</div>